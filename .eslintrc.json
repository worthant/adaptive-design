{
    "env": {
      "browser": true,
      "es2023": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 4],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"],
      "no-console": ["warn"],
      "eqeqeq": ["error", "always"],
      "curly": ["error", "multi-line"],
      "brace-style": ["error", "1tbs"]
    }
  }
  